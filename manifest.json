{
  "manifest_version": 3,
  "name": "Copy-Past Intelligently",
  "version": "1.0",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "clipboardRead"
  ],
  "action": {
    "default_icon": {
      "16": "./icon-16.png",
      "32": "./icon-32.png",
      "64": "./icon-64.png",
      "128": "./icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "settings.html"
}
